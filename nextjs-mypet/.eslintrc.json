{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [1, "never"],
        "quotes": [1, "single"],
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0,
        "react/no-unknown-property": 0
    }
}
